#include<bits/stdc++.h>
using namespace std;
string s;
int mp[26];

int main(){
    int t,p;
    int cs=0;
    while(1){
        scanf("%d",&t);
        if(t==0) break;
        int ans=0;
        for(int i=1;i<=t;i++){
            scanf("%d",&p);
            if(p==0) ans--;
            else ans++;
        }
        printf("Case %d: %d\n",++cs,ans);
    }
}
